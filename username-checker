import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner (System.in);
       int n=Integer.parseInt(sc.nextLine());
       
       while(n-- >0){
        System.out.println(sc.nextLine().matches("^[A-Za-z][A-Za-z0-9_]{7,29}$")?"Valid":"Invalid");
       }
    
    }
}
