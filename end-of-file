import java.io.*;
import java.util.*;
import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        for(int i=1; sc.hasNextLine(); i++){
        String line= sc.nextLine();
         System.out.println(i+" "+line);
    }
    // System.out.println();
        
}
}
